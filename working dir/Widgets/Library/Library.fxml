<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="root" prefHeight="150.0" prefWidth="150.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
<children><MenuBar fx:id="controlsBar" layoutX="28.0" layoutY="75.0">
  <menus>
    <Menu fx:id="addMenu" mnemonicParsing="false" text="+">
      <items><MenuItem mnemonicParsing="false" onAction="#addDirectory" text="Add directory" /><MenuItem mnemonicParsing="false" onAction="#addDirectoryNedit" text="Add directory and edit" />
        <MenuItem mnemonicParsing="false" onAction="#addFiles" text="Add files" /><MenuItem mnemonicParsing="false" onAction="#addFilesNedit" text="Add files and edit" />
      </items>
    </Menu>
    <Menu fx:id="remMenu" mnemonicParsing="false" text="-">
      <items>
        <MenuItem mnemonicParsing="false" onAction="#removeInvalid" text="Remove invalid items" />
      </items>
    </Menu>
  </menus>
</MenuBar>
</children></AnchorPane>
